{
  "name": "EpicWin",
  "action": {},
  "manifest_version": 3,
  "version": "1",
  "description": "Cette extension aide Ã  identifier les termes non inclusifs d'un site web, et propose des alternatives. Pour analyser une page, cliquer sur le bouton de l'extension.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["resources/dictionary.csv"],
      "matches": ["https://*/*"]
    }
  ],
  "icons": {
    "16": "icons/epicwin.png",
    "32": "icons/epicwin.png",
    "48": "icons/epicwin.png",
    "128": "icons/epicwin.png"
  }
}